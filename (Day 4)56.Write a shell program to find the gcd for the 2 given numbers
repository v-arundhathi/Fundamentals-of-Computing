#!/bin/bash

while true; do
    read a b
    gcd=$a
    if [ $b -lt $gcd ]; then
        gcd=$b
    fi
    while [ $gcd -ne 0 ]; do
        x=`expr $a % $gcd`
        y=`expr $b % $gcd`
        if [ $x -eq 0 -a $y -eq 0 ]; then
            echo "GCD ($a, $b) = $gcd"
        break
        fi
    done
done
